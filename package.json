{
  "name": "weaktuplemap",
  "version": "1.0.0",
  "description": "A WeakMap which accepts multiple objects as a key",
  "main": "dist/index.js",
  "jsnext:main": "index.js",
  "scripts": {
    "lint": "eslint ./index.js ./__tests__",
