  "license": "MPL-2.0",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "jest": "^17.0.3",
    "rimraf": "^2.5.4"
  }
}
